# Uncomment the next line to define a global platform for your project
platform :ios, '10.3'

target 'Villim' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Villim
  pod 'SnapKit', '~> 3.2.0'
  pod 'Alamofire', '~> 4.4'
  pod 'SwiftyJSON'
  pod 'Nuke', '~> 5.0'
  pod 'Toaster'
  pod 'GoogleMaps'
  pod 'Cosmos', '~> 11.0'
  pod 'FSCalendar', '~> 2.7'
  pod 'SwiftDate', '~> 4.0'
  pod 'PhoneNumberKit', '~> 1.3'
  pod 'pop', '~> 1.0'

  target 'VillimTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'VillimUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
