# Uncomment the next line to define a global platform for your project
platform :ios, '10.3'

target 'Villim' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Villim
  pod 'SnapKit', '~> 3.2.0'
  pod 'Alamofire', '~> 4.4'
  pod 'SwiftyJSON'
  pod 'NVActivityIndicatorView'
  pod 'Nuke', '~> 5.0'
  pod 'Toaster'
  pod 'GoogleMaps'
  pod 'Cosmos', '~> 11.0'

  target 'VillimTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'VillimUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
